{
  "name": "babel-preset-solfege",
  "version": "1.2.0",
  "description": "Babel preset to build SolfegeJS",
  "author": "neolao <contact@neolao.com>",
  "repository": "https://github.com/neolao/babel-preset-solfege",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "babel-preset",
    "solfege"
  ],
  "files": [
    "lib"
  ],
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-closure-elimination": "^1.1.16",
    "babel-plugin-syntax-async-generators": "^6.13.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-do-expressions": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "babel --out-dir lib src --copy-files"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "comments": false
  }
}
